   "source": [
    "dec = int(input('10진수를 입력하시오: '))\n",
    "convert = []\n",
    "while True:\n",
    "    m = dec // 2\n",
    "    r = dec % 2\n",
    "    convert.append(r)\n",
    "    if m == 0:\n",
    "        break\n",
    "    # ===== if\n",
    "    dec = m\n",
    "# ===== while\n",
    "for i in range(len(convert) -1, -1, -1):\n",
    "    print(convert[i], end='')\n",
    "print()"
   ]
